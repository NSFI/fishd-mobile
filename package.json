    "fishd-doc": "^1.2.0",
    "fishd-nuka-carousel": "^1.0.3",
    "lottie-web": "^5.7.3",
    "rc-animate": "^3.0.0",
    "rc-checkbox": "^2.0.3",
    "rc-field-form": "^1.22.0",
    "rc-rate": "^2.8.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-photo-view": "^0.5.1",
    "react-transition-group": "^4.4.1",
    "rmc-cascader": "^5.0.3",
    "rmc-date-picker": "6.0.8",
    "rmc-dialog": "1.0.1",
    "rmc-feedback": "^2.0.0",
