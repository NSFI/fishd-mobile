{
  "compilerOptions": {
    "typeRoots": ["./source/typings", "./node_modules/@types"],
    "moduleResolution": "node",
    "jsx": "react",
    "target": "es6",
    "strictNullChecks": true,
    "allowSyntheticDefaultImports":true,
    "noImplicitAny": false,
    "noUnusedParameters": false,
    "noUnusedLocals": false
  },
  "exclude": [
    "node_modules",
    "lib",
    "es"
  ],
  "compileOnSave": false
}
